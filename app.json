{
  "expo": {
    "name": "GridGenius",
    "slug": "sudokuapp",
    "version": "1.2.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-splash-screen",
      "expo-dev-client",
      "expo-av",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3940256099942544~3347511713",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash_v1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "ios": {
      "bundleIdentifier": "io.sekki.sudoku",
      "buildNumber": "2",
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash_v1.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "infoPlist": {
        "CFBundleDisplayName": "GridGenius Sudoku",
        "CFBundleShortVersionString": "1.2.0",
        "CFBundleVersion": "2",
        "GADApplicationIdentifier": "ca-app-pub-3940256099942544~1458002511",
        "UIStatusBarHidden": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "io.sekki.sudoku",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash_v1.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/splash_v1.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "5f565d19-71ba-4a49-af09-990b3aa33a19"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-3940256099942544~3347511713",
    "ios_app_id": "ca-app-pub-3940256099942544~1458002511"
  }
}
