{
  "expo": {
    "name": "GridGenius Sudoku",
    "slug": "sudokuapp",
    "version": "1.2.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-splash-screen",
      "expo-dev-client",
      "expo-av",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-7765897544369826~3669005398",
          "iosAppId": "ca-app-pub-7765897544369826~3669005398"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "ios": {
      "bundleIdentifier": "io.sekki.sudoku",
      "buildNumber": "2",
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "infoPlist": {
        "CFBundleDisplayName": "GridGenius Sudoku",
        "CFBundleShortVersionString": "1.2.0",
        "CFBundleVersion": "2",
        "GADApplicationIdentifier": "ca-app-pub-7765897544369826~3669005398",
        "UIStatusBarHidden": true
      }
    },
    "android": {
      "package": "io.sekki.sudoku",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
